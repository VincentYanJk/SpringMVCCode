<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>CustomDispatchSeverlet</servlet-name>
  	<display-name>DispatchServlet</display-name>
  	<description></description>
  	<servlet-class>com.pwc.springMVC.servlet.CustomDispatchSeverlet</servlet-class>
  	<init-param>
  		<param-name>basePackage</param-name>
  		<param-value>com.pwc.springMVC</param-value>
  		<description></description>
  	</init-param>
  	<load-on-startup>-1</load-on-startup> 
  </servlet>
  <servlet-mapping>
  	<servlet-name>CustomDispatchSeverlet</servlet-name>
  	<url-pattern>*.do</url-pattern>
  </servlet-mapping>
</web-app>
